    "num = int(input())\n",
    "for i in range(num):\n",
    "    line = input().split()\n",
    "    count = int(line[0])\n",
    "    s = line[1]\n",
    "    _str = ''\n",
    "    for j in s:\n",
    "        plus = j*count\n",
    "        _str += plus\n",
    "    print(_str)"
